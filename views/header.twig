				</svg>
			</button>
			<ul id="primary-navigation" aria-expanded="false">
				<li>
					<button aria-controls="submenu" aria-expanded="false" class="btn-submenu">
						{{ __('Mes services', 'fetedusleep') }}
						<svg class="icon-arrow"
							xmlns="http://www.w3.org/2000/svg"
							width="13.061"
							height="7.592"
							viewBox="0 0 13.061 7.592">
							<path id="Tracé_437"
								data-name="Tracé 437"
								d="M-20756-11033.383l6,6-6,6"
								transform="translate(-11020.853 20756.531) rotate(90)"
								stroke-width="1.5" />
						</svg>
					</button>
					{% if services %}
						<ul id="submenu" aria-expanded="false">
							{% for service in services %}
								<li class="item">
									{{
										service.meta('link') ?
											'<a href="' ~ service.meta('link').url ~ '" target="_blank" rel="noopener noreferrer">' :
											'<a href="' ~ service.link ~ '">'
									}}
										<div class="img-container desktop">
											{{ function('wp_get_attachment_image', service._thumbnail_id, 'medium_large') }}
										</div>
										<p class="item-title">
											{{ service.post_title }}
										</p>
									</a>
								</li>
							{% endfor %}
						</ul>
					{% endif %}
				</li>

				{# Mon livre #}
				<li>
					<a href="https://www.amazon.fr/dp/2702927858" target="_blank">Mon livre</a>
				</li>

