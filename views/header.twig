				<li>
					{% set quisuisje_page = function('get_page_by_path', 'qui-suis-je') %}
					{% if quisuisje_page %}
						{% set quisuisje_post_id = function('pll_get_post', quisuisje_page.ID) %}
						<a href="{{ function('get_the_permalink', quisuisje_post_id) }}">
							{{ __('Qui suis-je', 'fetedusleep') }}
						</a>
					{% endif %}
				</li>

				{# Contact #}
				<li>
					{% set contact_page = function('get_page_by_path', 'contact') %}
					{% if contact_page %}
						{% set contact_post_id = function('pll_get_post', contact_page.ID) %}
						<a href="{{ function('get_the_permalink', contact_post_id) }}">
							{{ __('Contact', 'fetedusleep') }}
						</a>
					{% endif %}
				</li>

				<li class="mobile">
